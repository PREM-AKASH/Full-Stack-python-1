   "source": [
    "def singleint(n):\n",
    "    while n >= 10:\n",
    "        n = sum(int(digit) for digit in str(n))\n",
    "    return n\n",
    "\n",
    "num = int(input(\"Enter a number: \"))\n",
    "print(\"Output: \",singleint(num))"
   ]
